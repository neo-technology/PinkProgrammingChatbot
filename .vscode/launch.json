{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flask: Run app.py",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0"
            },
            "args": ["run"],
            "jinja": true,
            "python": "${workspaceFolder}/.venv/bin/python"
        }
    ]
}
